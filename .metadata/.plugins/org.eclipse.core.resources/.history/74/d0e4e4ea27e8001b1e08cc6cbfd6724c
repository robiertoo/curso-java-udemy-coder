package lambdas;

public class Threads {
	public static void main(String[] args) {
		
		Runnable trabalho1 = new Trabalho1();
		Runnable trabalho2 = new Runnable() {
			
			@Override
			public void run() {
				// TODO Auto-generated method stub
				for (int i = 0; i < 100; i++) {
					System.out.println("Tarefa 02");
				}
			}
		};
		
		
	}
}
