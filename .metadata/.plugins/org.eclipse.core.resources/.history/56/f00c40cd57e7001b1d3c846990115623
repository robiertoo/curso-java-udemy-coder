package oo.heranca.desafio;

public class Ferrari extends Carro {
	Ferrari(int velocidadeMaxima) {
		super(velocidadeMaxima);
	}
	
	@Override
	boolean acelerar() {
		boolean acelerada1 = super.acelerar();
		boolean acelerada2 = super.acelerar();
		boolean acelerada3 = super.acelerar();
		
		return acelerada1 || acelerada2 || acelerada3;
	}
	
	@Override
	boolean frear() {
		boolean frenagem1 = super.frear();
		boolean frenagem2 = super.frear();
		boolean frenagem3 = super.frear();
		
		return frenagem1 || frenagem2 || frenagem3;
	}
}
