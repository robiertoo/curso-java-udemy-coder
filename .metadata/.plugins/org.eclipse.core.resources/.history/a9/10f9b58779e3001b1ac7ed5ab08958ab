package fundamentos;

public class NotacaoPonto {
	public static void main(String[] args) {
		double a = 2.3;
		String s = "Atalaia X";
		
		System.out.println(s);
		s = s.replace("X", "Jurubeba");
		System.out.println(s);
		s = s.toUpperCase();
		
		String numero = "3";
		System.out.println(numero);
		System.out.println(numero.toString());
	
	}
}
