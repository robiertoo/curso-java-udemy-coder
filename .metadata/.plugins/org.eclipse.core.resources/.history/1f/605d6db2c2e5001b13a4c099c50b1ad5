package classe;

public class DataTeste {
	public static void main(String[] args) {
		Data d1 = new Data();
		Data d2 = new Data();
		
		d1.dia = 31;
		d1.mes = 8;
		d1.ano = 1996;
		
		d2.dia = 21;
		d2.mes = 8;
		d2.ano = 1996;
		
		System.out.printf("Data 1: %d/%d/%d \n", d1.dia, d1.mes, d1.ano);
		System.out.printf("Data 2: %d/%d/%d", d2.dia, d2.mes, d2.ano);
	}
}
