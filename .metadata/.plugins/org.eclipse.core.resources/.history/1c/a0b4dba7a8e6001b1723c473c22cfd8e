package oo.composicao;

public class Carro {
	Motor motor = new Motor();
	
	void ligar() {
		motor.ligado = true;
	}
	
	void desligar() {
		motor.ligado = false;
	}
	
	boolean estaLigado() {
		return motor.ligado
	}
	
	void acelerar() {
//		if(motor.fatorInjecao < 3.0) {
			motor.fatorInjecao += 0.4;			
//		} else motor.fatorInjecao = 3;
	}
	
	void frear() {
//		if(motor.fatorInjecao > 0.5) {
			motor.fatorInjecao -= 0.4;			
//		} else motor.fatorInjecao = 0.5;
	}
	
}
